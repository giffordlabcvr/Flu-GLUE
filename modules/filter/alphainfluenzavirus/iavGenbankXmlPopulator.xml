<genbankXmlPopulator>

	<gbPrimaryAccession>
		<propertyPopulator property="gb_primary_accession"/>
	</gbPrimaryAccession>
	<gbAccessionVersion>
		<propertyPopulator property="gb_accession_version"/>
	</gbAccessionVersion>
	
	<!-- #### TOP LEVEL DATA -->	
	<gbLength>
		<propertyPopulator property="length"/>
	</gbLength>
	<gbUpdateDate>
		<propertyPopulator property="gb_update_date"/>
	</gbUpdateDate>
	<gbCreateDate>
		<propertyPopulator property="gb_create_date"/>
	</gbCreateDate>


	<!-- #### ORGANISM -->			
	<gbOrganism>
		<propertyPopulator property="full_name">
		</propertyPopulator>
	</gbOrganism>

	<!-- #### Exclude patent sequences etc-->	
	<gbDivision>
		<propertyPopulator property="lab_construct">
			<matchPattern>PATENT</matchPattern>
			<outputTemplate>true</outputTemplate>
		</propertyPopulator>
	</gbDivision>
	

	<!-- #### PUBMED ID -->			
	<gbReference>
		<gbRefPubmed>
			<propertyPopulator property="pubmed_id"/>
		</gbRefPubmed>
	</gbReference>
	
	
	<gbFeature key="source">

		<gbQualifier name="serotype">
			<propertyPopulator property="serotype"/>
		</gbQualifier>
	
		<gbQualifier name="strain">
			<propertyPopulator property="strain"/>
		</gbQualifier>
		
		<gbQualifier name="isolate">
			<propertyPopulator property="strain"/>
		</gbQualifier>
		
		
		<gbQualifier name="isolation_source">
			<propertyPopulator property="isolation_source"/>
		</gbQualifier>
		<gbQualifier name="lab_host">
			<propertyPopulator property="lab_host"/>
		</gbQualifier>
		
		
		<gbQualifier name="country">
			<propertyPopulator property="country"/>
			<propertyPopulator property="place_sampled">
				<matchPattern>^.*:(.*)$</matchPattern>
				<outputTemplate>${g1?trim}</outputTemplate>
			</propertyPopulator>
		</gbQualifier>

		<gbQualifier name="host">
			<propertyPopulator property="host_sci_name">
			
				<!-- #### Environmental -->																																									
				<valueConverter>
					<matchPattern>.*[eE]nvironment.*</matchPattern>
					<matchPattern>.*[eE]nviornment.*</matchPattern>
					<matchPattern>.*[eE]nviroment.*</matchPattern>
					<matchPattern>.*ENVIRONMENT.*</matchPattern>
					<matchPattern>ENV</matchPattern>
					<outputTemplate>Environment</outputTemplate>
				</valueConverter>


				<!-- #### Not known -->																																									
				<valueConverter>
					<matchPattern>.*Reverse Genetics.*</matchPattern>
					<matchPattern>.*reverse genetic.*</matchPattern>
					<matchPattern>.*U.*</matchPattern>
					<matchPattern>.*unknown.*</matchPattern>
					<outputTemplate>NK</outputTemplate>
				</valueConverter>


				<!-- #### Human -->																																									
				<valueConverter>
					<matchPattern>.*Homo [sS]apiens.*</matchPattern>
					<matchPattern>.*hom sapiens.*</matchPattern>
