delete project betainfluenza_filter

create project betainfluenza_filter "GLUE project for filtering betainfluenzavirus data" --minVersion 1.1.41

schema-project betainfluenza_filter

  run file glue/filter/betainfluenzavirus/ibvFilterSchemaExtensions.glue
  exit

project betainfluenza_filter

  # Create extension modules  
  run file glue/filter/betainfluenzavirus/ibvFilterModules.glue

  # Import sources 
  run file glue/filter/betainfluenzavirus/ibvFilterLoadSources.glue

  # Import data from GenBank XML
  module ibvFilterGenbankXmlPopulator populate
 
  # Import tabular data (e.g. corrections to GenBank)

  # Define references
  run file glue/filter/betainfluenzavirus/ibvFilterReferences.glue

  # Run segment recogniser
  run script glue/filter/betainfluenzavirus/doSegmentRecognition.js
    
  # Run genotyping procedure  
  run script glue/filter/betainfluenzavirus/doSegment4Genotying.js
    
  # Validate
  validate

  exit
  