delete project gammainfluenza_filter

create project gammainfluenza_filter "GLUE project for filtering gammainfluenzavirus data" --minVersion 1.1.41

schema-project gammainfluenza_filter

  run file glue/filter/gammainfluenzavirus/icvFilterSchemaExtensions.glue
  exit

project gammainfluenza_filter

  # Create extension modules  
  run file glue/filter/gammainfluenzavirus/icvFilterModules.glue

  # Import sources 
  run file glue/filter/gammainfluenzavirus/icvFilterLoadSources.glue

  # Import data from GenBank XML
  module icvFilterGenbankXmlPopulator populate
 
  # Import tabular data (e.g. corrections to GenBank)

  # Define references
  run file glue/filter/gammainfluenzavirus/icvFilterReferences.glue

  # Run segment recogniser
  run script glue/filter/gammainfluenzavirus/doSegmentRecognition.js
        
  # Validate
  validate

  exit
  