delete project deltainfluenza_filter

create project deltainfluenza_filter "GLUE project for filtering deltainfluenzavirus data" --minVersion 1.1.41

schema-project deltainfluenza_filter

  run file glue/filter/deltainfluenzavirus/idvFilterSchemaExtensions.glue
  exit

project deltainfluenza_filter

  # Create extension modules  
  run file glue/filter/deltainfluenzavirus/idvFilterModules.glue

  # Import sources 
  run file glue/filter/deltainfluenzavirus/idvFilterLoadSources.glue

  # Import data from GenBank XML
  module idvFilterGenbankXmlPopulator populate
 
  # Import tabular data (e.g. corrections to GenBank)

  # Define references
  run file glue/filter/deltainfluenzavirus/idvFilterReferences.glue

  # Run segment recogniser
  #run script glue/filter/deltainfluenzavirus/doSegmentRecognition.js
        
  # Validate
  validate

  exit
  