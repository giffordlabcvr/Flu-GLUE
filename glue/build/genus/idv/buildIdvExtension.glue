project flu

  # Create extension modules  
  run file glue/build/genus/idv/idvModules.glue

  # Import sources 
  run file glue/build/genus/idv/idvLoadSources.glue

  # Link imported sequences to isolate table 
  run script glue/build/genus/idv/idvLoadIsolateData.js

  # Import data from GenBank XML
  module genbankXmlPopulator populate

  # Define constrained alignment tree for extension project
  run file glue/build/genus/idv/idvAlignmentTree.glue
   
  # Validate
  validate

  exit
  