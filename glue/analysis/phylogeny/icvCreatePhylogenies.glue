  module fluPhyloUtility 
	reroot-phylogeny \
      --inputFile trees/icv_seg1.tre NEWICK_BOOTSTRAPS \
      --midpoint \
      --outputFile trees/icv_seg1MidpointRerooted.tree NEWICK_BOOTSTRAPS

