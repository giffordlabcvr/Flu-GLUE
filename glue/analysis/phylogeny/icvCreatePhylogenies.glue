  delete module raxmlPhylogenyGenerator
  delete module fluPhyloUtility
  delete module fluFigTreeAnnotationExporter
  
  create module -t raxmlPhylogenyGenerator
  create module -f modules/extension/fluPhyloUtility.xml
  create module -f modules/extension/fluFigTreeAnnotationExporter.xml

  module raxmlPhylogenyGenerator
    generate nucleotide phylogeny ICV_SEG1 -a -o trees/icv_seg1.tre NEWICK_BOOTSTRAPS 
    exit

