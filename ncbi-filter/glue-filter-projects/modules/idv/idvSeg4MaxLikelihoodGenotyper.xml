	<maxLikelihoodPlacerModuleName>idvSeg4MaxLikelihoodPlacer</maxLikelihoodPlacerModuleName>
	<useSingleReference>true</useSingleReference>

	<cladeCategory>
		<name>subtype</name>	
		<displayName>Subtype</displayName>	
		<whereClause>parent.name = 'AL_IDV_SEG4'</whereClause>
		<distanceScalingExponent>-3.0</distanceScalingExponent>
	    <distanceCutoff>0.015</distanceCutoff>
